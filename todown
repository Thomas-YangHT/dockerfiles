RUN wget  -c -t 20 http://surviving2023.com/linux/linux-headers-5.3.0-rc3-v8-gea837612b_5.3.0-rc3-v8-gea837612b-1_arm64.deb 
RUN wget  -c -t 20 https://github.com/dhruvvyas90/qemu-rpi-kernel/raw/master/kernel-qemu-4.4.34-jessie

